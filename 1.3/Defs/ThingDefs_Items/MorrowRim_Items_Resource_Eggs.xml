<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Mudcrab -->
	<ThingDef ParentName="EggFertBase">
		<defName>MorrowRim_Egg_MudcrabFertilized</defName>
		<label>mudcrab egg (fert.)</label>
		<description>A fertilized mudcrab egg. If all goes well, it should hatch into a baby mudcrab. Rather disgusting when eaten raw, but is tolerable cooked.</description>
		<graphicData>
		  <texPath>Things/Item/Resource/AnimalProductRaw/EggRound</texPath>
		  <color>(109,111,109)</color>
		  <drawSize>1.0</drawSize>
		</graphicData>
		<statBases>
		  <MarketValue>11</MarketValue>
		  <Nutrition>0.15</Nutrition>
		</statBases>
		<comps>
		  <li Class="CompProperties_Hatcher">
			<hatcherDaystoHatch>4</hatcherDaystoHatch>
			<hatcherPawn>MorrowRim_Mudcrab</hatcherPawn>
		  </li>
		</comps>
		<tradeTags>
			<li>AshlandRare</li>
		</tradeTags>
		<ingestible>
		  <specialThoughtDirect>MorrowRim_ateInsectEggDirect</specialThoughtDirect>
		  <specialThoughtAsIngredient>MorrowRim_ateInsectEggAsIngredient</specialThoughtAsIngredient>
		</ingestible>
	</ThingDef>
	
