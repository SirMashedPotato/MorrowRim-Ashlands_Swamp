<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dinosauria</li>
		</mods> 
				
		<match Class="PatchOperationSequence">
			<operations>
				
				<!-- add select animals to ashlands -->
						
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/ThingDef[defName="Baryonyx"]/race/wildBiomes</xpath>
					<value>
						<MorrowRim_AshSwamp>0.05</MorrowRim_AshSwamp>
					</value>
				</li>
				
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/ThingDef[defName="Spinosaurus"]/race/wildBiomes</xpath>
					<value>
						<MorrowRim_AshSwamp>0.05</MorrowRim_AshSwamp>
					</value>
				</li>

			</operations>
		</match>
	</Operation>
	
</Patch>

