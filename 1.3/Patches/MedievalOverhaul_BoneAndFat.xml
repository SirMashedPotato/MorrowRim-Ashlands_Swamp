<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Overhaul</li>
		</mods> 
				
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/ThingDef[defName="MorrowRim_SwampJelly"]</xpath>
					<value>
						<li Class="DankPyon.ButcherProperties">
							<hasBone>false</hasBone>
							<hasFat>false</hasFat>
						</li>
					</value>
				</li>	

				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/ThingDef[defName="MorrowRim_SwampStrider"]</xpath>
					<value>
						<li Class="DankPyon.ButcherProperties">
							<hasBone>false</hasBone>
							<hasFat>true</hasFat>
						</li>
					</value>
				</li>	

			</operations>
		</match>
	</Operation>
	
</Patch>

